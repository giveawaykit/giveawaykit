#giveaway_stats.advanced-stats
  #stats_popover_container.popover-container
  %section.panel.panel-default
    .row.m-l-none.m-r-none.bg-light.lter.kpis
      .col-sm-6.col-md-3.padder-v.b-r.b-light.kpi
        .stat.views.clearfix
          %span.fa-stack.fa-2x.pull-left.m-r-sm
            %i.fa.fa-circle.fa-stack-2x.text-danger
            %i.fa.fa-eye.fa-stack-1x.text-white
          %span.stat-container.clear
            %span.h3.block.m-t-xs
              %strong= giveaway.views
            %span.stat-title{ data: { toggle: "popover", container: "#stats_popover_container", placement: "auto top", html: "true", trigger: "click", title: "Views", content: "The total number of impressions the giveaway has recieved." } }
              %small.text-muted.text-uc Views
              %i.fa.fa-question-circle.text-muted
        .stat.viral-views.clearfix
          %span.fa-stack.fa-2x.pull-left.m-r-sm
          %span.stat-container.clear
            %span.h3.block.m-t-xs
              %strong= giveaway.viral_views
            %span.stat-title{ data: { toggle: "popover", container: "#stats_popover_container", placement: "auto top", html: "true", trigger: "click", title: "Viral Views", content: "The total number of impressions generated by Wall Posts, Requests, and External Shares of the giveaway." } }
              %small.text-muted.text-uc Viral Views
              %i.fa.fa-question-circle.text-muted
        .stat.uniques.clearfix
          %span.fa-stack.fa-2x.pull-left.m-r-sm
